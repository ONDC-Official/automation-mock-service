{
    "on_search":[
        {
            "_NAME_": "Buyer finder fees value is correct",
            "buyer_finder_fees": "$.message.catalog.providers[*].payments[*].tags[?(@.descriptor.code == 'BUYER_FINDER_FEES')].list[?(@.descriptor.code == 'BUYER_FINDER_FEES_PERCENTAGE')].value",
            "buyer_app_finder_fees": "$_EXTERNAL.buyer_app_finder_fees",
            "_RETURN_": "buyer_finder_fees equal to buyer_app_finder_fees"
        },
        {
            "_NAME_": "Vehicle category passed in is metro",
            "vehicle": "$.message.catalog.providers[*].fulfillments[*].vehicle.category",
            "vehicle_category": "$_EXTERNAL.vehicle_category",
            "_RETURN_": "vehicle_category all in vehicle"
        },
        {
            "_NAME_": "Start code matches the search call",
            "start_code": "$.message.catalog.providers[0].fulfillments[0].stops[?(@.type == 'START')].location.descriptor.code",
            "start": "$_EXTERNAL.start_code",
            "_RETURN_": "start_code all in start",
            "_CONTINUE_": "start is NULL check this"
        },
        {
            "_NAME_": "End code matches the search call",
            "end_code": "$.message.catalog.providers[0].fulfillments[0].stops[?(@.type == 'END')].location.descriptor.code",
            "end": "$_EXTERNAL.end_code",
            "_RETURN_": "end_cooe all in end",
            "_CONTINUE_": "end is NULL CHECK THIS"
        }
    ],
    "select": [
            {
            "_NAME_": "check id selected", 
			"attr": "$.message.order.items[*].id",
            "ids": "$._EXTERNAL.selected_ids",
			"_RETURN_": "attr all in ids"
            }
        ],
        "on_select": [
            {
            "_NAME_": "Category id is correctly mapped", 
			"attr": "$.message.order.items[*].category_ids",
            "category_ids": "$._EXTERNAL.category_ids",
			"_RETURN_": "attr all in category_ids"
            },
            {
            "_NAME_": "Fulfillments are correctly mapped", 
            "attr": "$.message.order.items[*].fulfillment_ids[*]",
            "fulfillments": "$.message.order.fulfillments[*].id",
            "_RETURN_": "attr all in fulfillments" 
            }
        ],
        "init": [
            {
                "_NAME_": "Category id is correctly mapped", 
                "attr": "$.message.order.payments[*].tags[?(@.descriptor.code=='BUYER_FINDER_FEES')].list[?(@.descriptor.code=='BUYER_FINDER_FEES_PERCENTAGE')].value",
                "buyer_finder_fees": "$._EXTERNAL.maximum_quantity NEED TO CHANGE",
                "_RETURN_": "attr equal buyer_finder_fees"
            },
            {
                "_NAME_": "Items sent in init and select are the same", 
                "attr": "$.message.order.items[*].id",
                "item_ids": "$._EXTERNAL.selected_ids",
                "_RETURN_": "attr equal item_ids" 
            }
        ],
        "on_init": [
            {
                "_NAME_": "Fulfillments are correctly mapped", 
                "attr": "$.message.order.items[*].fulfillment_ids[*]",
                "fulfillments": "$.message.order.fulfillments[*].id",
                "_RETURN_": "attr equal to fulfillments" 
            }
        ],
        "confirm": [
            {
                "_NAME_": "Items sent in init and confirm are the same", 
                "attr": "$.message.order.items[*].id",
                "item_ids": "$._EXTERNAL.selected_ids",
                "_RETURN_": "attr equal item_ids" 
            },
            {
                "_NAME_": "Items sent in init and confirm are the same", 
                "attr": "$.message.order.payments[*].params.amount",
                "price": "$._EXTERNAL.maximum_quantity NEED TO CHANGE",
                "_RETURN_": "attr equal price" 
            }
        ],
        "on_confirm": [
            {
                "_NAME_": "Price matches payment and quote.breakup", 
                "attr": "$.message.order.payments[*].params.amount",
                "price": "$.message.order.quote.price.value",
                "_RETURN_": "attr equal price" 
            },
            {
                "_NAME_": "Price is same as previous calls", 
                "attr": "$.message.order.quote.price.value",
                "price": "$._EXTERNAL.maximum_quantity NEED TO CHANGE",
                "_RETURN_": "attr equal price" 
            }
        ]
}